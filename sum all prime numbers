function sumPrimes(num) {
  let sum = 2;

  for(let i=2; i<=num; i++) {
            for (let j=2; j<i; j++) {
                   if (i%j==0) break;
                   if (j== i-1) {
                        sum+=i
                        console.log(i);
                        break;
                   }
                   
             }
       }
  
  return sum;
}

console.log(10); //17
